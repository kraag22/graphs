extends layout

block content
  h1 Chance to win


  div(class="col-xs-12 col-sm-10 col-sm-offset-1")
    ul(id="chance-list")

    span Martin's average chance to win in previous years:
      strong  #{martin_chance}
      | %

  script.
    var graph = new Graphs(!{data});
    graph.renderChance("chance-list");


